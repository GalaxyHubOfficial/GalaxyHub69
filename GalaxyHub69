elseif Game.PlaceId == 2577423786 then
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Superhero Simulator", "BloodTheme")
local AutofarmTab = Window:NewTab("AutoFarm")
local AutofarmSection = AutofarmTab:NewSection("Autofarm")
Section:NewButton("Coin Farm", "OP Use It", function(f)
   while wait(.1) do 
for i, v in pairs(game.Workspace.CoinSpawners:GetChildren()) do
wait (0.01)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
end
end
end)

AutofarmSection:NewKeybind("ToggleUI", "Toggles UI", Enum.KeyCode.t, function(t)
	Library:ToggleUI(t)
end)


else
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Mercenary Pack", "BloodTheme")
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Give Special Items")
MainSection:NewButton("Give Click TP", "Gives Click Teleport", function(g)
    mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Click Teleport"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack

end)

MainSection:NewButton("Enable e To Noclip", "lol", function(n)
    print('Loaded Noclip')
print('Press "E" to noclip')

noclip = false
game:GetService('RunService').Stepped:connect(function()
if noclip then
game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end
end)
plr = game.Players.LocalPlayer
mouse = plr:GetMouse()
mouse.KeyDown:connect(function(key)

if key == "e" then
noclip = not noclip
game.Players.LocalPlayer.Character.Humanoid:ChangeState(11)
end
end)
end)
MainSection:NewKeybind("ToggleUI", "Togles UI", Enum.KeyCode.F, function(t)
	Library:ToggleUI(t)
end)


end
